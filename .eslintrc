{
    "extends": ["plugin:prettier/recommended", "eslint:recommended"],
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "globals": {
        "document": true,
        "window": true,
        "location": true
    },
    "rules": {
        "no-unused-vars": 1,
        "no-console": 1
    }
}
